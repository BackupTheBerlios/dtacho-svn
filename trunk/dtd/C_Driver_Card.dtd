<!ELEMENT activityRecordLength ( #PCDATA )>
<!ELEMENT activityRecordDate ( #PCDATA )>
<!ELEMENT activityDailyPresenceCounter ( #PCDATA )>
<!ELEMENT activityDayDistance ( #PCDATA )>
<!ELEMENT activityChangeInfoSet ( activityChangeInfo+ )>
<!ELEMENT activityChangeInfo ( value, slot, drivingStatus, cardStatus, activity, time )>
<!ELEMENT value ( #PCDATA )>
<!ELEMENT slot ( #PCDATA )>
<!ELEMENT drivingStatus ( #PCDATA )>
<!ELEMENT cardStatus ( #PCDATA )>
<!ELEMENT activity ( #PCDATA )>
<!ELEMENT time ( #PCDATA )>

<!ELEMENT EF_Vehicles_Used ( cardVehiclesUsed )>
<!ELEMENT cardVehiclesUsed ( cardVehicleRecords )>
<!ELEMENT cardVehicleRecords ( cardVehicleRecord* )>
<!ELEMENT cardVehicleRecord ( vehicleOdometerBegin, vehicleOdometerEnd, vehicleFirstUse, vehicleLastUse, vehicleRegistration, vuDataBlockCounter )>
<!ELEMENT vehicleOdometerBegin ( #PCDATA )>
<!ELEMENT vehicleOdometerEnd ( #PCDATA )>
<!ELEMENT vehicleFirstUse ( #PCDATA )>
<!ELEMENT vehicleLastUse ( #PCDATA )>
<!ELEMENT vehicleRegistration ( vehicleRegistrationNation, vehicleRegistrationNumber )>
<!ELEMENT vuDataBlockCounter ( #PCDATA )>

<!ELEMENT EF_Places ( cardPlaceDailyWorkPeriod )>
<!ELEMENT cardPlaceDailyWorkPeriod ( placeRecords )>
<!ELEMENT placeRecords ( placeRecord* )>
<!ELEMENT placeRecord ( entryTime, entryTypeDailyWorkPeriod, dailyWorkPeriodCountry, dailyWorkPeriodRegion, vehicleOdometerValue )>
<!ELEMENT entryTime ( #PCDATA )>
<!ELEMENT entryTypeDailyWorkPeriod ( #PCDATA )>
<!ELEMENT dailyWorkPeriodCountry ( #PCDATA )>
<!ELEMENT dailyWorkPeriodRegion ( #PCDATA )>
<!ELEMENT vehicleOdometerValue ( #PCDATA )>

<!ELEMENT EF_Current_Usage ( cardCurrentUse? )>
<!ELEMENT cardCurrentUse ( sessionOpenTime, sessionOpenVehicle )>
<!ELEMENT sessionOpenTime ( #PCDATA )>
<!ELEMENT sessionOpenVehicle ( vehicleRegistrationNation, vehicleRegistrationNumber )>

<!ELEMENT EF_Control_Activity_Data ( cardControlActivityDataRecord? )>
<!ELEMENT cardControlActivityDataRecord ( controlType, controlTime, controlCardNumber, controlVehicleRegistration, controlDownloadPeriodBegin, controlDownloadPeriodEnd )>
<!ELEMENT controlType ( value, card_downloading, vu_downloading, display, printing )>
<!ELEMENT card_downloading ( #PCDATA )>
<!ELEMENT vu_downloading ( #PCDATA )>
<!ELEMENT display ( #PCDATA )>
<!ELEMENT printing ( #PCDATA )>
<!ELEMENT controlCardNumber ( cardType, cardIssuingMemberState, cardNumber )>
