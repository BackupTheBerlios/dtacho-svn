<!ELEMENT controlVehicleRegistration ( vehicleRegistrationNation, vehicleRegistrationNumber )>
<!ELEMENT controlTime ( #PCDATA )>
<!ELEMENT controlDownloadPeriodBegin ( #PCDATA )>
<!ELEMENT controlDownloadPeriodEnd ( #PCDATA )>

<!ELEMENT EF_Specific_Conditions ( specificConditionRecord* )>
<!ELEMENT specificConditionRecord ( entryTime, specificConditionType )>
<!ELEMENT specificConditionType ( #PCDATA )>
