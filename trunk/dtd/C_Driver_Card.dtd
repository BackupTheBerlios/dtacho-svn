<!ELEMENT cardIdentification ( cardIssuingMemberState, cardNumber, cardIssuingAuthorityName, cardIssueDate, cardValidityBegin, cardExpiryDate )>
<!ELEMENT cardIssuingMemberState ( #PCDATA )>
<!ELEMENT cardNumber ( ( driverIdentification, cardReplacementIndex, cardRenewalIndex ) | ( ownerIdentification, cardConsecutiveIndex, cardReplacementIndex, cardRenewalIndex ) )>
<!ELEMENT driverIdentification ( #PCDATA )>
<!ELEMENT cardReplacementIndex ( #PCDATA )>
<!ELEMENT cardRenewalIndex ( #PCDATA )>
<!ELEMENT cardIssuingAuthorityName ( codePage, name )>
<!ELEMENT codePage ( #PCDATA )>
<!ELEMENT name ( #PCDATA )>
<!ELEMENT cardIssueDate ( #PCDATA )>
<!ELEMENT cardValidityBegin ( #PCDATA )>
<!ELEMENT cardExpiryDate ( #PCDATA )>
<!ELEMENT driverCardHolderIdentification ( cardHolderName, cardHolderBirthDate, cardHolderPreferredLanguage )>
<!ELEMENT cardHolderName ( holderSurname, holderFirstNames )>
<!ELEMENT holderSurname ( codePage, name )>
<!ELEMENT holderFirstNames ( codePage, name )>
<!ELEMENT cardHolderBirthDate ( year, month, day )>
<!ELEMENT year ( #PCDATA )>
<!ELEMENT month ( #PCDATA )>
<!ELEMENT day ( #PCDATA )>
<!ELEMENT cardHolderPreferredLanguage ( #PCDATA )>
<!ELEMENT ownerIdentification ( #PCDATA )>
<!ELEMENT cardConsecutiveIndex ( #PCDATA )>

<!ELEMENT EF_Driving_Licence_Info ( cardDrivingLicenceInformation )>
<!ELEMENT cardDrivingLicenceInformation ( drivingLicenceIssuingAuthority, drivingLicenceIssuingNation, drivingLicenceNumber )>
<!ELEMENT drivingLicenceIssuingAuthority ( codePage, name )>
<!ELEMENT drivingLicenceIssuingNation ( #PCDATA )>
<!ELEMENT drivingLicenceNumber ( #PCDATA )>

<!ELEMENT EF_Events_Data ( cardEventData )>
<!ELEMENT cardEventData ( cardEventRecords+ )>
<!ELEMENT cardEventRecords ( cardEventRecord* )>
<!ELEMENT cardEventRecord ( eventType, eventBeginTime, eventEndTime, eventVehicleRegistration )>
<!ELEMENT eventType ( #PCDATA )>
<!ELEMENT eventBeginTime ( #PCDATA )>
<!ELEMENT eventEndTime ( #PCDATA )>

<!ELEMENT eventVehicleRegistration ( vehicleRegistrationNation, vehicleRegistrationNumber )>
<!ELEMENT vehicleRegistrationNation ( #PCDATA )>
<!ELEMENT vehicleRegistrationNumber ( codePage, vehicleRegNumber )>
<!ELEMENT vehicleRegNumber ( #PCDATA )>

<!ELEMENT EF_Faults_Data ( cardFaultData )>
<!ELEMENT cardFaultData ( cardFaultRecords+ )>
<!ELEMENT cardFaultRecords ( cardFaultRecord* )>
<!ELEMENT cardFaultRecord ( faultType, faultBeginTime, faultEndTime, faultVehicleRegistration )>
<!ELEMENT faultType ( #PCDATA )>
<!ELEMENT faultBeginTime ( #PCDATA )>
<!ELEMENT faultEndTime ( #PCDATA )>

<!ELEMENT faultVehicleRegistration ( vehicleRegistrationNation, vehicleRegistrationNumber )>

<!ELEMENT EF_Driver_Activity_Data ( cardDriverActivity )>
