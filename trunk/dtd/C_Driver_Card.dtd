<!ELEMENT noOfEventsPerType ( #PCDATA )>
<!ELEMENT noOfFaultsPerType ( #PCDATA )>
<!ELEMENT activityStructureLength ( #PCDATA )>
<!ELEMENT noOfCardVehicleRecords ( #PCDATA )>
<!ELEMENT noOfCardPlaceRecords ( #PCDATA )>

<!ELEMENT EF_Card_Certificate ( cardCertificate )>
