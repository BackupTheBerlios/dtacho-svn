<!ELEMENT monthYear ( #PCDATA )>
<!ELEMENT type ( #PCDATA )>
<!ELEMENT manufacturerCode ( #PCDATA )>
<!ELEMENT cardApprovalNumber ( #PCDATA )>
<!ELEMENT cardPersonaliserID ( #PCDATA )>
<!ELEMENT embedderIcAssemblerId ( #PCDATA )>
<!ELEMENT icIdentifier ( #PCDATA )>

<!ELEMENT EF_IC ( cardChipIdentification )>
<!ELEMENT cardChipIdentification ( icSerialNumber, icManufacturingReferences )>
